using System;

public class HelloWorld
{
    public static void Main(string[] args)
    {
       Person myObj = new Person();
        myObj.Name = "Liam";
        Console.WriteLine(myObj.Name);
    }
    class Person
    {
      private string name; // field
      public string Name   // property
      {
        get { return name; }
        set { name = value; }
      }
    }
}
