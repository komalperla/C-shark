using System;
public class program
{
    
    public static void Main(string[] args)
    {
        for(int counter=0;counter <10;counter++)
        {
            
            if(counter%2==0)
            {
                Console.WriteLine("we skip number 3!");
                continue;
            }
            Console.WriteLine(counter);
        }
       Console.ReadKey();
    }
    
    
}
