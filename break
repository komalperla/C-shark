using System;


public class program
{
    
    public static void Main(string[] args)
    {
        for(int counter=0;counter <10;counter++)
        {
            Console.WriteLine(counter);
            if(counter==3)
            {
                Console.WriteLine("at 3 we stop !");
                break;
            }
        }
       Console.ReadKey();
    }
    
    
}
